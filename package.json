		"viewsContainers": {
			"activitybar": [
				{
					"id": "uiw-views",
					"title": "uiw",
					"icon": "./assets/logo.svg"
				}
			]
		},
		"views": {
			"uiw-views": [
				{
					"id": "uiw.view.ecosystem",
					"name": "component"
				}
			]
		},
		"menus": {
			"editor/context": [
				{
					"command": "uiw.cmd.help",
					"when": "editorFocus",
					"group": "navigation"
				}
			],
			"view/title": [
				{
					"command": "uiw.cmd.ecoRefreshNode",
					"when": "view == uiw.view.ecosystem",
					"group": "navigation@2"
				},
				{
					"command": "uiw.cmd.Document",
					"when": "view == uiw.view.ecosystem",
					"group": "navigation@1"
				}
			],
			"view/item/context": []
		},
		"snippets": [
			{
				"language": "javascript",
				"path": "./snippets/react.json"
			},
			{
				"language": "javascriptreact",
				"path": "./snippets/react.json"
			},
			{
				"language": "typescriptreact",
				"path": "./snippets/react.json"
			}
		]
	},
	"keybindings": [
		{
			"command": "uiw.cmd.help",
			"key": "ctrl+f10",
			"mac": "cmd+f10",
			"when": "editorTextFocus"
		}
	],
	"scripts": {
		"build": "vsce package",
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./"
	},
